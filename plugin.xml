<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>
            <pluginName>ContactForm</pluginName>

			<pluginDescription>Simple ContactForm plugin</pluginDescription>

            <pluginVersion>1.0</pluginVersion>
            <pluginRevision>1</pluginRevision>
            <pluginBuildTimestamp>0</pluginBuildTimestamp>

            <pluginIframeSrc>/plugin/ContactForm/admin/index</pluginIframeSrc>

			<pluginClassName>\ContactForm\Plugin</pluginClassName>

			<pluginIncludePaths>
				<path>/ContactForm/lib</path>
			</pluginIncludePaths>

			<pluginNamespaces>
				<namespace>ContactForm</namespace>
			</pluginNamespaces>

			<pluginJsPaths>
                <path>/plugins/ContactForm/static/js/startup.js</path>
				<path>/plugins/ContactForm/static/js/form.js</path>
			</pluginJsPaths>

			<pluginCssPaths>
                <path>/plugins/ContactForm/static/css/style.css</path>
			</pluginCssPaths>
		</plugin>
</zend-config>
